cificDate(i)\"> <span class=\"select-date\">{{ item }}</span> </v-list-item-title>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n\n        <div class=\"grafic\">\n          <apexchart width=\"100%\" height=\"400\" type=\"area\" :options=\"options\" :series=\"series\"></apexchart>\n        </div>\n      </div>\n   </div>\n\n    <!-- <div class=\"box-data\">\n      <div v-for=\"dado, index in dados\" :key=\"index\">\n        <div class=\"card-details\">\n          <p class=\"title\">{{ dado.title }}</p>\n          <p class=\"information\">{{ dado.information }}</p>\n        </div>\n      </div>\n    </div> -->\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data () {\n    return {\n      dados: [\n        { title: 'Vendas', information: 60 },\n        { title: 'Estoque', information: 332 },\n        { title: 'Clientes', information: 43 },\n        { title: 'Faturamento', information: 'R$5.320,00' }\n      ],\n      typeGrafic: 'Anual',\n      dateGrafic: [2021, 2020],\n      dateSelect: 0,\n      options: {\n        chart: {\n          id: 'vuechart-history'\n        },\n        xaxis: {\n          categories: ['01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12']\n        }\n      },\n      series: [\n        {\n          name: 'faturamento',\n          data: [30, 40, 45, 50, 49, 60, 70, 40, 21, 100, 30, 10]\n        }\n      ]\n    }\n  },\n  methods: {\n    specificDate (index) {\n      this.dateSelect = this.dateGrafic[index]\n\n      if (this.dateSelect === 2020) {\n        console.log('2020')\n        this.series = [{\n          name: 'faturamento',\n          data: [10, 40, 50, 50, 50, 55, 65, 45, 30, 55, 60, 70]\n        }]\n      } else {\n        console.log('2021')\n        this.series = [{\n          name: 'faturamento',\n          data: [30, 40, 45, 50, 49, 60, 70, 40, 21, 100, 30, 10]\n        }]\n      }\n    }\n  },\n  created () {\n    this.dateSelect = this.dateGrafic[0]\n  }\n}\n</script>\n"],